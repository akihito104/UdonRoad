<merge xmlns:android="http://schemas.android.com/apk/res/android">
<!--<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content">-->
    <ImageView
        android:id="@+id/indicator_up"
        android:layout_width="@dimen/small_user_icon"
        android:layout_height="@dimen/small_user_icon"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        />
    <ImageView
        android:id="@+id/indicator_right"
        android:layout_width="@dimen/small_user_icon"
        android:layout_height="@dimen/small_user_icon"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        />
    <ImageView
        android:id="@+id/indicator_down"
        android:layout_width="@dimen/small_user_icon"
        android:layout_height="@dimen/small_user_icon"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        />
    <ImageView
        android:id="@+id/indicator_left"
        android:layout_width="@dimen/small_user_icon"
        android:layout_height="@dimen/small_user_icon"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        />
    <ImageView
        android:id="@+id/indicator_selected"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="gone"
        />
<!--</RelativeLayout>-->
</merge>